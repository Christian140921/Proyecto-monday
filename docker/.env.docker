# Variables para Docker Compose
# Copia este archivo como .env en la raíz del proyecto

# Token de Strapi (crear en Admin > Settings > API Tokens)
STRAPI_API_TOKEN=

# Sincronización
STRAPI_SYNC=true
STRAPI_SYNC_DELETE=false

# Strapi secrets (puedes generar valores aleatorios)
STRAPI_APP_KEYS=key1,key2,key3,key4
STRAPI_API_TOKEN_SALT=randomsalt123
STRAPI_ADMIN_JWT_SECRET=adminjwtsecret456
STRAPI_TRANSFER_TOKEN_SALT=transfersalt789
STRAPI_JWT_SECRET=jwtsecret123abc
